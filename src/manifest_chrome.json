{
  "description": "video resumer",
  "manifest_version": 3,
  "name": "video resumer",
  "version": "1.0.3",
  "homepage_url": "https://github.com/divinity76/video-resumer",
  "icons": {
    "48": "icons/page-48.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "icons/page-32.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "resumer.js"
      ]
    }
  ]
}